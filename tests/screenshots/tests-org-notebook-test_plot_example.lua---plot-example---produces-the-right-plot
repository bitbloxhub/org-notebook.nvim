01|#+NAME: plot_example                                                            
02|#+BEGIN_SRC ts :results value graphics replace                                  
03|import { document } from "register-window"                                      
04|import * as Plot from "@observablehq/plot"                                      
05|import d from "vega-datasets"                                                   
06|                                                                                
07|const p = await d["penguins.json"]()                                            
08|                                                                                
09|Plot.plot({                                                                     
10|        marks: [                                                                
11|                Plot.dot(p, {                                                   
12|                        x: "Beak Depth (mm)",                                   
13|                        y: "Beak Length (mm)",                                  
14|                        fill: "Island",                                         
15|                }),                                                             
16|        ],                                                                      
17|        document,                                                               
18|})                                                                              
19|#+END_SRC                                                                       
20|                                                                                
