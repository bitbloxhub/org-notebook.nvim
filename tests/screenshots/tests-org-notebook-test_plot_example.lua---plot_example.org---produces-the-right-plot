01|#+BEGIN_SRC ts :results value graphics replace                                  
02|import { document } from "register-window"                                      
03|import * as Plot from "@observablehq/plot"                                      
04|import d from "vega-datasets"                                                   
05|                                                                                
06|const p = await d["penguins.json"]()                                            
07|                                                                                
08|Plot.plot({                                                                     
09|        marks: [                                                                
10|                Plot.dot(p, {                                                   
11|                        x: "Beak Depth (mm)",                                   
12|                        y: "Beak Length (mm)",                                  
13|                        fill: "Island",                                         
14|                }),                                                             
15|        ],                                                                      
16|        document,                                                               
17|})                                                                              
18|#+END_SRC                                                                       
